<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="109dp"
            android:layout_width="114dp"
            android:id="@+id/view6"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.921"
            motion:layout_constraintHorizontal_bias="0.946">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@android:color/holo_blue_bright" />
        </Constraint>
        <Constraint
            android:layout_height="156dp"
            android:layout_width="111dp"
            android:id="@+id/imageView5"
            motion:layout_constraintStart_toStartOf="@+id/view6"
            motion:layout_constraintEnd_toEndOf="@+id/view6"
            motion:layout_constraintTop_toTopOf="@+id/view6"
            motion:layout_constraintBottom_toBottomOf="@+id/view6"
            android:alpha="0" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintEnd_toEndOf="@+id/view6"
            motion:layout_constraintStart_toStartOf="@+id/view6"
            motion:layout_constraintTop_toTopOf="@+id/view6"
            android:layout_marginTop="8dp"
            motion:layout_constraintHorizontal_bias="1.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="584dp"
            android:layout_width="381dp"
            android:id="@+id/view6"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_editor_absoluteX="9dp"
            motion:layout_editor_absoluteY="-7dp">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@android:color/holo_red_dark" />
        </Constraint>
        <Constraint
            android:layout_height="161dp"
            android:layout_width="155dp"
            android:id="@+id/imageView5"
            motion:layout_constraintTop_toTopOf="@+id/view6"
            motion:layout_constraintEnd_toEndOf="@+id/view6"
            motion:layout_constraintStart_toStartOf="@+id/view6"
            motion:layout_constraintBottom_toBottomOf="@+id/view6"
            motion:layout_constraintVertical_bias="0.464"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintEnd_toEndOf="@+id/view6"
            motion:layout_constraintStart_toStartOf="@+id/view6"
            motion:layout_constraintTop_toTopOf="@+id/view6"
            motion:layout_constraintBottom_toBottomOf="@+id/view6"
            motion:layout_constraintVertical_bias="0.031"
            motion:layout_constraintHorizontal_bias="0.946" />
    </ConstraintSet>
</MotionScene>