<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/textView2"
            android:layout_width="83dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView"
            motion:layout_constraintVertical_bias="0.381"
            android:rotation="0"
            android:layout_marginStart="40dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/view4" />
        <Constraint
            android:id="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="350dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="@+id/view4"
            motion:layout_constraintTop_toTopOf="@+id/view4"
            android:alpha="0" />
        <Constraint
            android:id="@+id/view4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="434dp"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="542dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="410dp"
            motion:layout_constraintTop_toBottomOf="@+id/view4"
            android:id="@+id/scrollView2" />
    </ConstraintSet>
</MotionScene>