<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_width="326dp"
            android:layout_height="88dp"
            android:id="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.454"
            motion:layout_editor_absoluteX="85dp"
            motion:layout_editor_absoluteY="111dp" />
        <Constraint
            android:id="@+id/a"
            android:layout_width="395dp"
            android:layout_height="194dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.103"
            motion:layout_constraintHorizontal_bias="0.34">
            <CustomAttribute
                motion:attributeName="textColor"
                motion:customColorValue="@color/black" />
        </Constraint>
        <Constraint
            android:layout_height="155dp"
            android:layout_width="202dp"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/a"
            android:layout_marginTop="16dp"
            motion:layout_constraintHorizontal_bias="0.028" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/button"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="554dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="61dp"
            android:layout_width="410dp"
            android:id="@+id/view3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.909"
            motion:layout_constraintHorizontal_bias="0.0"
            android:alpha="0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_width="75dp"
            android:layout_height="64dp"
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/button"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.91"
            motion:layout_constraintHorizontal_bias="0.0" >
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@android:color/holo_green_light" />
        </Constraint>
        <Constraint
            android:layout_height="82dp"
            android:layout_width="109dp"
            android:id="@+id/imageView4"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.904"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/a"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/button"
            motion:layout_constraintVertical_bias="0.663"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="521dp"
            android:alpha="1"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            android:layout_height="64dp"
            android:layout_width="match_parent"
            android:id="@+id/view3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/button"
            motion:layout_constraintVertical_bias="0.923"
            motion:layout_constraintHorizontal_bias="0.0"
            android:alpha="1">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@android:color/holo_red_light" />
        </Constraint>
    </ConstraintSet>
</MotionScene>