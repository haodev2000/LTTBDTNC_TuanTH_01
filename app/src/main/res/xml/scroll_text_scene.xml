<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:motionInterpolator="linear"

        motion:duration="250">
        <OnSwipe
            motion:dragDirection="dragDown"
            motion:touchAnchorId="@+id/background"
            motion:touchAnchorSide="bottom" />

        <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="150dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView13"
            motion:layout_constraintEnd_toEndOf="@+id/view10"
            motion:layout_constraintStart_toStartOf="@+id/view10"
            motion:layout_constraintTop_toTopOf="@+id/view10"
            motion:layout_constraintHorizontal_bias="0.912"
            motion:layout_constraintBottom_toBottomOf="@+id/view10" />
        <Constraint
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rotation="-90"
            motion:layout_constraintTop_toTopOf="@+id/imageView13"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView13"
            motion:layout_constraintEnd_toStartOf="@+id/imageView13"
            android:layout_marginEnd="48dp"
            android:layout_marginRight="48dp" />
        <Constraint
            motion:layout_constraintVertical_bias="0.002"
            android:layout_height="150dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="412dp"
            android:id="@+id/view10"
            android:alpha="0">
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@android:color/holo_blue_light" />
        </Constraint>
        <Constraint
            android:layout_height="450dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView10"
            motion:layout_constraintTop_toBottomOf="@+id/view10"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/view10"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            android:alpha="1" />
        <Constraint
            android:id="@+id/textView9"
            android:layout_width="130dp"
            android:layout_height="45dp"
            android:rotation="0"
            motion:layout_constraintTop_toTopOf="@+id/imageView13"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView13"
            motion:layout_constraintEnd_toStartOf="@+id/imageView13"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:layout_height="60dp"
            android:layout_width="260dp"
            android:id="@+id/imageView13"
            motion:layout_constraintTop_toTopOf="@+id/view10"
            motion:layout_constraintBottom_toBottomOf="@+id/view10"
            motion:layout_constraintEnd_toEndOf="@+id/view10"
            motion:layout_constraintStart_toStartOf="@+id/view10"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.915"
            android:alpha="0" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="540dp"
            android:id="@+id/textView10"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/view10"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toEndOf="@id/textView10" />
    </ConstraintSet>
</MotionScene>